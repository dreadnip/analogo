{
    "name": "dreadnip/analogo",
    "description": "Clear opcache through php-fpm",
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.2",
        "hollodotme/fast-cgi-client": "^3.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0",
        "phpstan/phpstan": "1.8",
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Analogo\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4" : {
            "Analogo\\Test\\": "tests/"
        }
    },
    "bin": ["analogo"]
}
